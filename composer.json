{
    "name": "cleaniquecoders/laravel-observers",
    "description": "A collection of observer classes that can be use in your Laravel application.",
    "keywords": ["package", "laravel", "observer", "hashids", "reference", "hashslug"],
    "license": "MIT",
    "authors": [
        {
            "name": "Nasrul Hazim",
            "email": "nasrulhazim.m@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "CleaniqueCoders\\LaravelObservers\\": "src/"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "CleaniqueCoders\\LaravelObservers\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.0|^8.1",
        "cleaniquecoders/laravel-helper": "^4.0",
        "hashids/hashids": "^4.0",
        "illuminate/support": "^9.0|^10.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "8.*",
        "pestphp/pest": "^2.0",
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "CleaniqueCoders\\LaravelObservers\\LaravelObserversServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "format": "vendor/bin/pint",
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
